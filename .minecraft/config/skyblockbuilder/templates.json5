{
  // The template which will be used for spawn only
  // Example: 
  // {
  //     "name": "default",
  //     "desc": "",
  //     "file": "default.nbt",
  //     "spawns": "default",
  //     "direction": "south"
  // }
  "spawn": null,
  
  "spawns": {
    "default": [
      [ 57, 3, 57 ]
    ]
  },
  
  // The list of templates being available. The first entry is the default template.
  "templates": [
    {
      "name": "default",
      "desc": "",
      "file": "default.nbt",
      "spawns": "default",
      "direction": "south"
    },
    {
      "name": "only grass block",
      "desc": "",
      "file": "only_grass_block.nbt",
      "spawns": "default",
      "direction": "south"
    }
  ]
}
